MODULE sup_mux_lhs (loopLiveIn, loopLiveIn_valid, iterLiveOut, iterLiveOut_valid, oldContinue, oldContinue_valid, iterLiveIn_ready)


-- input and output
DEFINE iterLiveIn := ndw_out_iterLiveIn.outs;
DEFINE iterLiveIn_valid := ndw_out_iterLiveIn.outs_valid;
DEFINE ndw_out_iterLiveIn_outs_ready := iterLiveIn_ready;
DEFINE ndw_in_loopLiveIn_ins := loopLiveIn;
DEFINE ndw_in_loopLiveIn_ins_valid := loopLiveIn_valid;
DEFINE loopLiveIn_ready := ndw_in_loopLiveIn.ins_ready;
DEFINE ndw_in_iterLiveOut_ins := iterLiveOut;
DEFINE ndw_in_iterLiveOut_ins_valid := iterLiveOut_valid;
DEFINE iterLiveOut_ready := ndw_in_iterLiveOut.ins_ready;
DEFINE ndw_in_oldContinue_ins := oldContinue;
DEFINE ndw_in_oldContinue_ins_valid := oldContinue_valid;
