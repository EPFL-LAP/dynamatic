#include "elastic_miter_sup_gamma_lhs_sup_gamma_rhs.smv"


MODULE bool_input(nReady0, max_tokens)
  VAR outs : boolean;
  VAR counter : 0..31;
  FROZENVAR exact_tokens : 0..max_tokens;
  ASSIGN
    init(counter) := 0;
    next(counter) := case
      nReady0 & counter < exact_tokens : counter + 1;
      TRUE : counter;
    esac;

  -- make sure outs is persistent
  ASSIGN
    next(outs) := case
      outs_valid & !nReady0 : outs;
      TRUE : {TRUE, FALSE};
    esac;

  DEFINE outs_valid := counter < exact_tokens;

MODULE sink_main (ins_valid)
  DEFINE ins_ready   := TRUE;

MODULE main

  VAR seq_generator_a1 : bool_input(elastic_miter_sup_gamma_lhs_sup_gamma_rhs.a1_ready, 1);
  VAR seq_generator_a2 : bool_input(elastic_miter_sup_gamma_lhs_sup_gamma_rhs.a2_ready, 1);
  VAR seq_generator_c1 : bool_input(elastic_miter_sup_gamma_lhs_sup_gamma_rhs.c1_ready, 1);
  VAR seq_generator_c2 : bool_input(elastic_miter_sup_gamma_lhs_sup_gamma_rhs.c2_ready, 1);

  VAR elastic_miter_sup_gamma_lhs_sup_gamma_rhs : elastic_miter_sup_gamma_lhs_sup_gamma_rhs (seq_generator_a1.outs, seq_generator_a1.outs_valid, seq_generator_a2.outs, seq_generator_a2.outs_valid, seq_generator_c1.outs, seq_generator_c1.outs_valid, seq_generator_c2.outs, seq_generator_c2.outs_valid, sink_EQ_b.ins_ready);

  VAR sink_EQ_b : sink_main(elastic_miter_sup_gamma_lhs_sup_gamma_rhs.EQ_b_valid);

INVARSPEC (elastic_miter_sup_gamma_lhs_sup_gamma_rhs.EQ_b_valid -> elastic_miter_sup_gamma_lhs_sup_gamma_rhs.EQ_b);
CTLSPEC AF (AG ((elastic_miter_sup_gamma_lhs_sup_gamma_rhs.lhs_in_buf_a1.debug_counter.counter = elastic_miter_sup_gamma_lhs_sup_gamma_rhs.rhs_in_buf_a1.debug_counter.counter) & (elastic_miter_sup_gamma_lhs_sup_gamma_rhs.lhs_in_buf_a2.debug_counter.counter = elastic_miter_sup_gamma_lhs_sup_gamma_rhs.rhs_in_buf_a2.debug_counter.counter) & (elastic_miter_sup_gamma_lhs_sup_gamma_rhs.lhs_in_buf_c1.debug_counter.counter = elastic_miter_sup_gamma_lhs_sup_gamma_rhs.rhs_in_buf_c1.debug_counter.counter) & (elastic_miter_sup_gamma_lhs_sup_gamma_rhs.lhs_in_buf_c2.debug_counter.counter = elastic_miter_sup_gamma_lhs_sup_gamma_rhs.rhs_in_buf_c2.debug_counter.counter) & (!elastic_miter_sup_gamma_lhs_sup_gamma_rhs.lhs_out_bl_b.outs_valid) & (!elastic_miter_sup_gamma_lhs_sup_gamma_rhs.rhs_out_bl_b.outs_valid)));

