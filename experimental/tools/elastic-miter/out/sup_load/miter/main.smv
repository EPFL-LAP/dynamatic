#include "elastic_miter_sup_load_lhs_sup_load_rhs.smv"


MODULE bool_input(nReady0, max_tokens)
  VAR outs : boolean;
  VAR counter : 0..31;
  FROZENVAR exact_tokens : 0..max_tokens;
  ASSIGN
    init(counter) := 0;
    next(counter) := case
      nReady0 & counter < exact_tokens : counter + 1;
      TRUE : counter;
    esac;

  -- make sure outs is persistent
  ASSIGN
    next(outs) := case
      outs_valid & !nReady0 : outs;
      TRUE : {TRUE, FALSE};
    esac;

  DEFINE outs_valid := counter < exact_tokens;

MODULE sink_main (ins_valid)
  DEFINE ins_ready   := TRUE;

MODULE main

  VAR seq_generator_Addr_in : bool_input(elastic_miter_sup_load_lhs_sup_load_rhs.Addr_in_ready, 3);
  VAR seq_generator_Cond : bool_input(elastic_miter_sup_load_lhs_sup_load_rhs.Cond_ready, 3);

  VAR elastic_miter_sup_load_lhs_sup_load_rhs : elastic_miter_sup_load_lhs_sup_load_rhs (seq_generator_Addr_in.outs, seq_generator_Addr_in.outs_valid, seq_generator_Cond.outs, seq_generator_Cond.outs_valid, sink_EQ_Data_out.ins_ready);

  VAR sink_EQ_Data_out : sink_main(elastic_miter_sup_load_lhs_sup_load_rhs.EQ_Data_out_valid);

INVARSPEC (elastic_miter_sup_load_lhs_sup_load_rhs.EQ_Data_out_valid -> elastic_miter_sup_load_lhs_sup_load_rhs.EQ_Data_out);
CTLSPEC AF (AG ((elastic_miter_sup_load_lhs_sup_load_rhs.lhs_in_buf_Addr_in.debug_counter.counter = elastic_miter_sup_load_lhs_sup_load_rhs.rhs_in_buf_Addr_in.debug_counter.counter) & (elastic_miter_sup_load_lhs_sup_load_rhs.lhs_in_buf_Cond.debug_counter.counter = elastic_miter_sup_load_lhs_sup_load_rhs.rhs_in_buf_Cond.debug_counter.counter) & (!elastic_miter_sup_load_lhs_sup_load_rhs.lhs_out_bl_Data_out.outs_valid) & (!elastic_miter_sup_load_lhs_sup_load_rhs.rhs_out_bl_Data_out.outs_valid)));

INVAR  seq_generator_Addr_in.exact_tokens = seq_generator_Cond.exact_tokens & seq_generator_Addr_in.exact_tokens <= elastic_miter_sup_load_lhs_sup_load_rhs.out_nds_Data_out.exact_tokens ;
