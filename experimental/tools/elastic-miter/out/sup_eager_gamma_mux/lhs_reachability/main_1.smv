#include "sup_eager_gamma_mux_lhs.smv"


MODULE bool_input_exact(nReady0, exact_tokens)
  VAR outs : boolean;
  VAR counter : 0..31;
  ASSIGN
    init(counter) := 0;
    next(counter) := case
      nReady0 & counter < exact_tokens : counter + 1;
      TRUE : counter;
    esac;

  -- make sure outs is persistent
  ASSIGN
    next(outs) := case
      outs_valid & !nReady0 : outs;
      TRUE : {TRUE, FALSE};
    esac;

  DEFINE outs_valid := counter < exact_tokens;

MODULE sink_main (ins_valid)
  DEFINE ins_ready   := TRUE;

MODULE main

  VAR seq_generator_A_in : bool_input_exact(sup_eager_gamma_mux_lhs.A_in_ready, 1);
  VAR seq_generator_B_in : bool_input_exact(sup_eager_gamma_mux_lhs.B_in_ready, 1);
  VAR seq_generator_ctrl : bool_input_exact(sup_eager_gamma_mux_lhs.ctrl_ready, 1);

  VAR sup_eager_gamma_mux_lhs : sup_eager_gamma_mux_lhs (seq_generator_A_in.outs, seq_generator_A_in.outs_valid, seq_generator_B_in.outs, seq_generator_B_in.outs_valid, seq_generator_ctrl.outs, seq_generator_ctrl.outs_valid, sink_res.ins_ready);

  VAR sink_res : sink_main(sup_eager_gamma_mux_lhs.res_valid);

