#include "elastic_miter_resolver_lhs_resolver_rhs.smv"


MODULE bool_input(nReady0, max_tokens)
  VAR outs : boolean;
  VAR counter : 0..31;
  FROZENVAR exact_tokens : 0..max_tokens;
  ASSIGN
    init(counter) := 0;
    next(counter) := case
      nReady0 & counter < exact_tokens : counter + 1;
      TRUE : counter;
    esac;

  -- make sure outs is persistent
  ASSIGN
    next(outs) := case
      outs_valid & !nReady0 : outs;
      TRUE : {TRUE, FALSE};
    esac;

  DEFINE outs_valid := counter < exact_tokens;

MODULE sink_main (ins_valid)
  DEFINE ins_ready   := TRUE;

MODULE main

  VAR seq_generator_Actual : bool_input(elastic_miter_resolver_lhs_resolver_rhs.Actual_ready, 3);
  VAR seq_generator_Generated : bool_input(elastic_miter_resolver_lhs_resolver_rhs.Generated_ready, 3);

  VAR elastic_miter_resolver_lhs_resolver_rhs : elastic_miter_resolver_lhs_resolver_rhs (seq_generator_Actual.outs, seq_generator_Actual.outs_valid, seq_generator_Generated.outs, seq_generator_Generated.outs_valid, sink_EQ_Confirm.ins_ready);

  VAR sink_EQ_Confirm : sink_main(elastic_miter_resolver_lhs_resolver_rhs.EQ_Confirm_valid);

INVARSPEC (elastic_miter_resolver_lhs_resolver_rhs.EQ_Confirm_valid -> elastic_miter_resolver_lhs_resolver_rhs.EQ_Confirm);
CTLSPEC AF (AG ((elastic_miter_resolver_lhs_resolver_rhs.lhs_in_buf_Actual.inner_elastic_fifo.head = elastic_miter_resolver_lhs_resolver_rhs.rhs_in_buf_Actual.inner_elastic_fifo.head) & (elastic_miter_resolver_lhs_resolver_rhs.lhs_in_buf_Actual.inner_elastic_fifo.tail = elastic_miter_resolver_lhs_resolver_rhs.rhs_in_buf_Actual.inner_elastic_fifo.tail) & (elastic_miter_resolver_lhs_resolver_rhs.lhs_in_buf_Generated.inner_elastic_fifo.head = elastic_miter_resolver_lhs_resolver_rhs.rhs_in_buf_Generated.inner_elastic_fifo.head) & (elastic_miter_resolver_lhs_resolver_rhs.lhs_in_buf_Generated.inner_elastic_fifo.tail = elastic_miter_resolver_lhs_resolver_rhs.rhs_in_buf_Generated.inner_elastic_fifo.tail) & (!elastic_miter_resolver_lhs_resolver_rhs.lhs_out_buf_Confirm.outs_valid) & (!elastic_miter_resolver_lhs_resolver_rhs.rhs_out_buf_Confirm.outs_valid)));

