#include "elastic_miter_interpInduction_lhs_interpInduction_rhs.smv"


MODULE bool_input(nReady0, max_tokens)
  VAR outs : boolean;
  VAR counter : 0..31;
  FROZENVAR exact_tokens : 0..max_tokens;
  ASSIGN
    init(counter) := 0;
    next(counter) := case
      nReady0 & counter < exact_tokens : counter + 1;
      TRUE : counter;
    esac;

  -- make sure outs is persistent
  ASSIGN
    next(outs) := case
      outs_valid & !nReady0 : outs;
      TRUE : {TRUE, FALSE};
    esac;

  DEFINE outs_valid := counter < exact_tokens;

MODULE sink_main (ins_valid)
  DEFINE ins_ready   := TRUE;

MODULE main

  VAR seq_generator_short_in : bool_input(elastic_miter_interpInduction_lhs_interpInduction_rhs.short_in_ready, 3);
  VAR seq_generator_long_in : bool_input(elastic_miter_interpInduction_lhs_interpInduction_rhs.long_in_ready, 3);

  VAR elastic_miter_interpInduction_lhs_interpInduction_rhs : elastic_miter_interpInduction_lhs_interpInduction_rhs (seq_generator_short_in.outs, seq_generator_short_in.outs_valid, seq_generator_long_in.outs, seq_generator_long_in.outs_valid, sink_EQ_B_out.ins_ready);

  VAR sink_EQ_B_out : sink_main(elastic_miter_interpInduction_lhs_interpInduction_rhs.EQ_B_out_valid);

INVARSPEC (elastic_miter_interpInduction_lhs_interpInduction_rhs.EQ_B_out_valid -> elastic_miter_interpInduction_lhs_interpInduction_rhs.EQ_B_out);
CTLSPEC AF (AG ((elastic_miter_interpInduction_lhs_interpInduction_rhs.lhs_in_buf_short_in.debug_counter.counter = elastic_miter_interpInduction_lhs_interpInduction_rhs.rhs_in_buf_short_in.debug_counter.counter) & (elastic_miter_interpInduction_lhs_interpInduction_rhs.lhs_in_buf_long_in.debug_counter.counter = elastic_miter_interpInduction_lhs_interpInduction_rhs.rhs_in_buf_long_in.debug_counter.counter) & (!elastic_miter_interpInduction_lhs_interpInduction_rhs.lhs_out_bl_B_out.outs_valid) & (!elastic_miter_interpInduction_lhs_interpInduction_rhs.rhs_out_bl_B_out.outs_valid)));

