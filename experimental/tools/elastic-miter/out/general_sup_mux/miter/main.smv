#include "elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.smv"


MODULE bool_input(nReady0, max_tokens)
  VAR outs : boolean;
  VAR counter : 0..31;
  FROZENVAR exact_tokens : 0..max_tokens;
  ASSIGN
    init(counter) := 0;
    next(counter) := case
      nReady0 & counter < exact_tokens : counter + 1;
      TRUE : counter;
    esac;

  -- make sure outs is persistent
  ASSIGN
    next(outs) := case
      outs_valid & !nReady0 : outs;
      TRUE : {TRUE, FALSE};
    esac;

  DEFINE outs_valid := counter < exact_tokens;

MODULE sink_main (ins_valid)
  DEFINE ins_ready   := TRUE;

MODULE main

  VAR seq_generator_sel : bool_input(elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.sel_ready, 10);
  VAR seq_generator_ctrl1 : bool_input(elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.ctrl1_ready, 10);
  VAR seq_generator_ctrl2 : bool_input(elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.ctrl2_ready, 10);
  VAR seq_generator_a_in : bool_input(elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.a_in_ready, 10);
  VAR seq_generator_b_in : bool_input(elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.b_in_ready, 10);

  VAR elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs : elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs (seq_generator_sel.outs, seq_generator_sel.outs_valid, seq_generator_ctrl1.outs, seq_generator_ctrl1.outs_valid, seq_generator_ctrl2.outs, seq_generator_ctrl2.outs_valid, seq_generator_a_in.outs, seq_generator_a_in.outs_valid, seq_generator_b_in.outs, seq_generator_b_in.outs_valid, sink_EQ_res.ins_ready);

  VAR sink_EQ_res : sink_main(elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.EQ_res_valid);

INVARSPEC (elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.EQ_res_valid -> elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.EQ_res);
CTLSPEC AF (AG ((elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.lhs_in_buf_sel.debug_counter.counter = elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.rhs_in_buf_sel.debug_counter.counter) & (elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.lhs_in_buf_ctrl1.debug_counter.counter = elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.rhs_in_buf_ctrl1.debug_counter.counter) & (elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.lhs_in_buf_ctrl2.debug_counter.counter = elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.rhs_in_buf_ctrl2.debug_counter.counter) & (elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.lhs_in_buf_a_in.debug_counter.counter = elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.rhs_in_buf_a_in.debug_counter.counter) & (elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.lhs_in_buf_b_in.debug_counter.counter = elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.rhs_in_buf_b_in.debug_counter.counter) & (!elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.lhs_out_bl_res.outs_valid) & (!elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.rhs_out_bl_res.outs_valid)));

INVAR  seq_generator_ctrl1.exact_tokens = seq_generator_a_in.exact_tokens & seq_generator_ctrl2.exact_tokens = seq_generator_b_in.exact_tokens ;

FAIRNESS (!elastic_miter_general_sup_mux_lhs_general_sup_mux_rhs.lhs_fork_ctrl1.outs_0_valid)
