MODULE bool_input(nReady0, max_tokens)
  VAR outs : boolean;
  VAR counter : 0..31;
  FROZENVAR exact_tokens : 0..max_tokens;
  ASSIGN
    init(counter) := 0;
    next(counter) := case
      nReady0 & counter < exact_tokens : counter + 1;
      TRUE : counter;
    esac;

  -- make sure outs is persistent
  ASSIGN
    next(outs) := case
      outs_valid & !nReady0 : outs;
      TRUE : {TRUE, FALSE};
    esac;

  DEFINE outs_valid := counter < exact_tokens;

MODULE sink_main (ins_valid)
  DEFINE ins_ready   := TRUE;

MODULE main

  VAR seq_generator_arg : bool_input(elastic_miter_extension2_lhs_extension2_rhs.arg_ready, 31);

  VAR elastic_miter_extension2_lhs_extension2_rhs : elastic_miter_extension2_lhs_extension2_rhs (seq_generator_arg.outs, seq_generator_arg.outs_valid, sink_EQ_res.ins_ready);

  VAR sink_EQ_res : sink_main(elastic_miter_extension2_lhs_extension2_rhs.EQ_res_valid);

INVARSPEC (elastic_miter_extension2_lhs_extension2_rhs.EQ_res_valid -> elastic_miter_extension2_lhs_extension2_rhs.EQ_res);
CTLSPEC AF (AG ((elastic_miter_extension2_lhs_extension2_rhs.lhs_in_buf_arg.debug_counter.counter = elastic_miter_extension2_lhs_extension2_rhs.rhs_in_buf_arg.debug_counter.counter) & (!elastic_miter_extension2_lhs_extension2_rhs.lhs_out_bl_res.outs_valid) & (!elastic_miter_extension2_lhs_extension2_rhs.rhs_out_bl_res.outs_valid)));

INVAR  seq_generator_arg.exact_tokens <= elastic_miter_extension2_lhs_extension2_rhs.out_nds_res.exact_tokens ;
