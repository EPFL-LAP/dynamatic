MODULE bool_input(nReady0, max_tokens)
  VAR outs : boolean;
  VAR counter : 0..31;
  FROZENVAR exact_tokens : 0..max_tokens;
  ASSIGN
    init(counter) := 0;
    next(counter) := case
      nReady0 & counter < exact_tokens : counter + 1;
      TRUE : counter;
    esac;

  -- make sure outs is persistent
  ASSIGN
    next(outs) := case
      outs_valid & !nReady0 : outs;
      TRUE : {TRUE, FALSE};
    esac;

  DEFINE outs_valid := counter < exact_tokens;

MODULE sink_main (ins_valid)
  DEFINE ins_ready   := TRUE;

MODULE main

  VAR seq_generator_A_in : bool_input(elastic_miter_introduce_ident_interpolator_lhs_introduce_ident_interpolator_rhs.A_in_ready, 1);

  VAR elastic_miter_introduce_ident_interpolator_lhs_introduce_ident_interpolator_rhs : elastic_miter_introduce_ident_interpolator_lhs_introduce_ident_interpolator_rhs (seq_generator_A_in.outs, seq_generator_A_in.outs_valid);


CTLSPEC AF (AG ((!elastic_miter_introduce_ident_interpolator_lhs_introduce_ident_interpolator_rhs.rhs_in_buf_A_in.outs_valid)));

