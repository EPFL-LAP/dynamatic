#include "elastic_miter_unify_sup_lhs_unify_sup_rhs.smv"


MODULE bool_input(nReady0, max_tokens)
  VAR outs : boolean;
  VAR counter : 0..31;
  FROZENVAR exact_tokens : 0..max_tokens;
  ASSIGN
    init(counter) := 0;
    next(counter) := case
      nReady0 & counter < exact_tokens : counter + 1;
      TRUE : counter;
    esac;

  -- make sure outs is persistent
  ASSIGN
    next(outs) := case
      outs_valid & !nReady0 : outs;
      TRUE : {TRUE, FALSE};
    esac;

  DEFINE outs_valid := counter < exact_tokens;

MODULE sink_main (ins_valid)
  DEFINE ins_ready   := TRUE;

MODULE main

  VAR seq_generator_A_in : bool_input(elastic_miter_unify_sup_lhs_unify_sup_rhs.A_in_ready, 1);
  VAR seq_generator_Cond1 : bool_input(elastic_miter_unify_sup_lhs_unify_sup_rhs.Cond1_ready, 1);
  VAR seq_generator_Cond2 : bool_input(elastic_miter_unify_sup_lhs_unify_sup_rhs.Cond2_ready, 1);

  VAR elastic_miter_unify_sup_lhs_unify_sup_rhs : elastic_miter_unify_sup_lhs_unify_sup_rhs (seq_generator_A_in.outs, seq_generator_A_in.outs_valid, seq_generator_Cond1.outs, seq_generator_Cond1.outs_valid, seq_generator_Cond2.outs, seq_generator_Cond2.outs_valid, sink_EQ_B_out.ins_ready);

  VAR sink_EQ_B_out : sink_main(elastic_miter_unify_sup_lhs_unify_sup_rhs.EQ_B_out_valid);

INVARSPEC (elastic_miter_unify_sup_lhs_unify_sup_rhs.EQ_B_out_valid -> elastic_miter_unify_sup_lhs_unify_sup_rhs.EQ_B_out);
CTLSPEC AF (AG ((elastic_miter_unify_sup_lhs_unify_sup_rhs.lhs_in_buf_A_in.debug_counter.counter = elastic_miter_unify_sup_lhs_unify_sup_rhs.rhs_in_buf_A_in.debug_counter.counter) & (elastic_miter_unify_sup_lhs_unify_sup_rhs.lhs_in_buf_Cond1.debug_counter.counter = elastic_miter_unify_sup_lhs_unify_sup_rhs.rhs_in_buf_Cond1.debug_counter.counter) & (elastic_miter_unify_sup_lhs_unify_sup_rhs.lhs_in_buf_Cond2.debug_counter.counter = elastic_miter_unify_sup_lhs_unify_sup_rhs.rhs_in_buf_Cond2.debug_counter.counter) & (!elastic_miter_unify_sup_lhs_unify_sup_rhs.lhs_out_bl_B_out.outs_valid) & (!elastic_miter_unify_sup_lhs_unify_sup_rhs.rhs_out_bl_B_out.outs_valid)));

INVAR  seq_generator_A_in.exact_tokens = seq_generator_Cond1.exact_tokens ;
