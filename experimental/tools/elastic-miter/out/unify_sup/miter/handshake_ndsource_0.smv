-- handshake_ndsource_0 : ndsource(None)


MODULE handshake_ndsource_0(result_ready)
  VAR val : boolean;
  ASSIGN
  init(val) := {TRUE, FALSE};
  next(val) := case
    transfer : {TRUE, FALSE};
    TRUE : val;
  esac;
  DEFINE
  transfer := result_ready;
  // output
  result := val;
  result_valid := TRUE;

