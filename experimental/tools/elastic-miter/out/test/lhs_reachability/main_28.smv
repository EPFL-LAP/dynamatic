#include "test_lhs.smv"


MODULE ctrl_input_exact(nReady0, exact_tokens)
  VAR counter : 0..31;
  ASSIGN
    init(counter) := 0;
    next(counter) := case
      nReady0 & counter < exact_tokens : counter + 1;
      TRUE : counter;
    esac;

  DEFINE outs_valid := counter < exact_tokens;

MODULE sink_main (ins_valid)
  DEFINE ins_ready   := TRUE;

MODULE main

  VAR seq_generator_A_in : ctrl_input_exact(test_lhs.A_in_ready, 28);

  VAR test_lhs : test_lhs (seq_generator_A_in.outs_valid);


