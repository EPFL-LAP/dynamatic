#include "elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.smv"


MODULE bool_input(nReady0, max_tokens)
  VAR outs : boolean;
  VAR counter : 0..31;
  FROZENVAR exact_tokens : 0..max_tokens;
  ASSIGN
    init(counter) := 0;
    next(counter) := case
      nReady0 & counter < exact_tokens : counter + 1;
      TRUE : counter;
    esac;

  -- make sure outs is persistent
  ASSIGN
    next(outs) := case
      outs_valid & !nReady0 : outs;
      TRUE : {TRUE, FALSE};
    esac;

  DEFINE outs_valid := counter < exact_tokens;

MODULE sink_main (ins_valid)
  DEFINE ins_ready   := TRUE;

MODULE main

  VAR seq_generator_in1 : bool_input(elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.in1_ready, 2);
  VAR seq_generator_in2 : bool_input(elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.in2_ready, 2);
  VAR seq_generator_sel : bool_input(elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.sel_ready, 2);

  VAR elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs : elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs (seq_generator_in1.outs, seq_generator_in1.outs_valid, seq_generator_in2.outs, seq_generator_in2.outs_valid, seq_generator_sel.outs, seq_generator_sel.outs_valid, sink_EQ_out.ins_ready);

  VAR sink_EQ_out : sink_main(elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.EQ_out_valid);

INVARSPEC (elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.EQ_out_valid -> elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.EQ_out);
CTLSPEC AF (AG ((elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.lhs_in_buf_in1.inner_counter.counter = elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.rhs_in_buf_in1.inner_counter.counter) & (elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.lhs_in_buf_in2.inner_counter.counter = elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.rhs_in_buf_in2.inner_counter.counter) & (elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.lhs_in_buf_sel.inner_counter.counter = elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.rhs_in_buf_sel.inner_counter.counter) & (!elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.lhs_out_bl_out.outs_valid) & (!elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.rhs_out_bl_out.outs_valid)));

INVAR  seq_generator_in1.exact_tokens >= seq_generator_sel.exact_tokens & seq_generator_in2.exact_tokens >= seq_generator_sel.exact_tokens ;
INVAR  elastic_miter_mux_to_logic_lhs_mux_to_logic_rhs.out_nds_out.exact_tokens >= seq_generator_sel.exact_tokens ;
