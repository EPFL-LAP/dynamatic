MODULE tehb(ins, ins_valid, outs_ready)
VAR inner_tehb : tehb_dataless(ins_valid, outs_ready);
VAR data : DATA_TYPE;
ASSIGN
init(data) := 0;
next(data) := ins_ready & ins_valid & !outs_ready ? ins : data;
DEFINE ins_ready := inner_tehb.ins_ready;
DEFINE outs := tehb_dataless.full ? data : ins;

VAR data : DATA_TYPE;
ASSIGN
init(data) := 0;
next(data) := ins_ready & ins_valid & !outs_ready ? ins : data;
